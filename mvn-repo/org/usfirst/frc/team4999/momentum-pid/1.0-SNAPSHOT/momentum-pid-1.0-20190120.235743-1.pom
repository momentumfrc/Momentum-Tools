<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.usfirst.frc.team4999</groupId>
  <artifactId>momentum-pid</artifactId>
  <packaging>jar</packaging>
  <version>1.0-SNAPSHOT</version>
  <name>momentum-pid</name>
  <url>http://maven.apache.org</url>
   
  <properties>
    <maven.compiler.source>8</maven.compiler.source>
    <maven.compiler.target>8</maven.compiler.target>
  </properties>
  
  <repositories>
    <repository>
      <id>wpilib</id>
      <name>Wpilib Plugin Repository</name>
      <url>http://first.wpi.edu/FRC/roborio/maven/release/</url>
    </repository>
  </repositories>
    
  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>edu.wpi.first.wpilibj</groupId>
      <artifactId>wpilibj-java</artifactId>
      <version>2019.2.1</version>
    </dependency>
  </dependencies>
  
  <distributionManagement>
    <repository>
        <id>github</id>
        <name>Momentum Github Repository</name>
        <url>file://${project.build.directory}/../../mvn-repo</url>
    </repository>
    <downloadUrl>https://raw.githubusercontent.com/momentumfrc/Momentum-PID/master/mvn-repo</downloadUrl>
  </distributionManagement>
  
</project>
